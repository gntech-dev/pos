{
  "name": "pos-system",
  "version": "1.0.2",
  "private": true,
  "engines": {
    "node": ">=20.19.6",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "db:seed": "tsx database/prisma/seed.ts",
    "db:migrate": "prisma migrate dev --schema=database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=database/prisma/schema.prisma",
    "prepare": "node -e \"try { require('husky') } catch(e) { console.log('Husky not available in production - skipping git hooks setup') }\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --findRelatedTests --passWithNoTests"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.4",
    "@types/speakeasy": "^2.0.10",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "jose": "^6.1.3",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "next": "^15.1.0",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.11",
    "papaparse": "^5.5.3",
    "prisma": "^5.22.0",
    "puppeteer": "^24.34.0",
    "rate-limiter-flexible": "^9.0.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.21.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
